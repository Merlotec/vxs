{
  "name": "voxelsim-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:wasm": "cd ../voxelsim-renderer && wasm-pack build --target web && xcopy /E /I /Y pkg ..\\voxelsim-frontend\\wasm",
    "clean": "rmdir /S /Q dist node_modules wasm\\*.js wasm\\*.wasm wasm\\*.d.ts 2>nul || true"
  },
  "keywords": [
    "voxelsim",
    "renderer",
    "wasm",
    "bevy"
  ],
  "author": "",
  "license": "ISC",
  "description": "VoxelSim web-based renderer frontend",
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@vitejs/plugin-react": "^5.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vite": "^7.1.11"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  }
}
